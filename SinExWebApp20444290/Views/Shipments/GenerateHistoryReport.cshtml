@model SinExWebApp20444290.ViewModels.ShipmentsReportViewModel

@{
    ViewBag.Title = "Shipment History";
}

<h2>Shipment History</h2>
@if (Request.IsAuthenticated)
{
    @Html.Partial("_ShipmentsSearchPartial", Model.Shipment)
    @Html.Partial("_ShipmentsListPartial", Model.Shipments)
    @Html.Partial("_ShipmentsReportViewPagingPartial", Model.Shipments)
}
else
{
    <p style="color: red;">Please log in to browse shipment history.</p>
}
